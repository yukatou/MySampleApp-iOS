  pod 'Moya'
  #pod 'ChameleonFramework/Swift'


  target 'MySampleAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MySampleAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
